{
  "name": "Informacao",
  "plural": "informacoes",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "mensagem": {
      "type": "string",
      "required": true
    },
    "responsavel": {
      "type": "string",
      "required": true
    },
    "sala": {
      "type": "string"
    },
    "turma": {
      "type": "string"
    },
    "orgao": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "date",
      "required": true
    },
    "validade": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "turmas": {
      "type": "hasMany",
      "model": "Turma",
      "foreignKey": "turmaId",
      "required": false
    },
    "responsavel": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "usuarioId",
      "required": true
    }
  },
  "acls": [],
  "methods": {}
}
